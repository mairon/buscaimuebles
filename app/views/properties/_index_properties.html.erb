<p id="notice"><%= notice %></p>

<h1>Listing Properties</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Area</th>
      <th>Price</th>
      <th>Description</th>
      <th>Property type</th>
      <th>Interest</th>
      <th>Status</th>
      <th>User</th>
      <th>Bed rooms</th>
      <th>Bath rooms</th>
      <th>Address</th>
      <th>County</th>
      <th>State</th>
      <th>City</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Video url</th>
      <th>Property option</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @properties.each do |property| %>
      <tr>
        <td><%= property.name %></td>
        <td><%= property.area %></td>
        <td><%= property.price %></td>
        <td><%= property.description %></td>
        <td><%= property.property_type_id %></td>
        <td><%= property.interest %></td>
        <td><%= property.status %></td>
        <td><%= property.user_id %></td>
        <td><%= property.bed_rooms %></td>
        <td><%= property.bath_rooms %></td>
        <td><%= property.address %></td>
        <td><%= property.county_id %></td>
        <td><%= property.state_id %></td>
        <td><%= property.city_id %></td>
        <td><%= property.latitude %></td>
        <td><%= property.longitude %></td>
        <td><%= property.video_url %></td>
        <td><%= property.property_option_id %></td>
        <td><%= link_to 'Show', property %></td>
        <td><%= link_to 'Edit', edit_property_path(property) %></td>
        <td><%= link_to 'Destroy', property, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Property', new_property_path %>
