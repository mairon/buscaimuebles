<p id="notice"><%= notice %></p>

<h1>Listing Property Types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @property_types.each do |property_type| %>
      <tr>
        <td><%= property_type.name %></td>
        <td><%= property_type.status %></td>
        <td><%= link_to 'Edit', edit_admin_property_type_path(property_type) %></td>
        <td><%= link_to 'Destroy', [:admin, property_type], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Property type', new_admin_property_type_path %>
